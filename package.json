  },
  "build": {
    "productName": "WanderAgent",
    "appId": "com.wanderagent.app",
    "asar": true,
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "files": [
      "electron/**/*",
      "dist/public/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "dist/public",
        "to": "dist/public"
      }
    ]
  }
