{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "main": "electron/main.cjs",
  "description": "WanderAgent - Travel planning app",
  "author": "Roshan Rateria",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "vite build",
    "build:client:local": "node ./scripts/inject-client-env.mjs",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "cap:init": "npx cap init WanderAgent com.wanderagent.app --web-dir=dist/public && npx cap add android",
    "apk": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"npm run build:client:local; npx cap sync android; cd android; .\\gradlew.bat assembleDebug\"",
    "apk:server": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$env:VITE_LOCAL_ONLY='false'; npm run build:client; npx cap sync android; cd android; .\\gradlew.bat assembleDebug\"",
    "apk:open": "npx cap open android",
    "clear": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Write-Host 'Cleaning Android build artifacts...'; $ErrorActionPreference='SilentlyContinue'; Remove-Item -Recurse -Force android\\app\\build; Remove-Item -Recurse -Force android\\build; Remove-Item -Recurse -Force android\\.gradle; Remove-Item -Recurse -Force android\\capacitor-cordova-android-plugins; Remove-Item -Recurse -Force android\\app\\src\\main\\assets; Write-Host 'Done.'\"",
    "set": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"npm run build:client:local; npx cap sync android; cd android; .\\gradlew.bat clean\"",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5000 && electron .\"",
    "electron:kill": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"try { Get-Process WanderAgent -ErrorAction SilentlyContinue | Stop-Process -Force } catch {}; try { Remove-Item -Recurse -Force release\\win-unpacked -ErrorAction SilentlyContinue } catch {}\"",
    "electron:build": "npm run electron:kill && cross-env VITE_LOCAL_ONLY=true VITE_LOCAL=true npm run build:client:local && electron-builder --win nsis"
  },
  "dependencies": {
    "@capacitor-community/http": "^1.4.1",
    "@capacitor/core": "^6.1.2",
    "@capacitor/filesystem": "^5.2.2",
